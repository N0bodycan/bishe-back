        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.homework.mapper.AdminMapper">

    <select id="getAdminByUsername" resultType="com.cy.homework.entity.Admin">
        select * from admin where username = #{username}
